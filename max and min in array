#include<stdio.h>
#define maxi 1000
int main()
{
    int i,num,sum=0;
    int arr[maxi];
    int max,min;
    scanf("%d",&num);
    for(i=0;i<num;i++)
    {
        scanf("%d",&arr[i]);
    }
    max=arr[0];
    min=arr[0];
    for(i=1;i<num;i++)
    {
      if(arr[i]>max)
      {
          max=arr[i];
      }
      if(arr[i]<min)
      {
          min=arr[i];
      }
    }
     printf("%d\n",max);
     printf("%d\n",min);
     return 0;
}
