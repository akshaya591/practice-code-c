#include<stdio.h>
#include<string.h>
int main()
{
    char a[100];
    fgets(a,sizeof(a),stdin);
    
    int l=strlen(a);
    for(int i=0;i<l;i++)
    {
        char c=a[i];
        if(!(c=='a'||c=='e'||c=='i'||c=='o'||c=='u'||c=='A'||c=='E'||c=='I'||c=='O'||c=='U'))
        {
            printf("%c",c);
        }
    }
}
