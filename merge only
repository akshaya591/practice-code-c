#include<stdio.h>
int main()
{
    int n1,n2;
    scanf("%d",&n1);
    int arr1[n1];
    for(int i=0;i<n1;i++)
    {
        scanf("%d",&arr1[i]);
    }
    scanf("%d",&n2);
    int arr2[n2];
    for(int j=0;j<n2;j++)
    {
        scanf("%d",&arr2[j]);
    }
    int n3=n1+n2;
    int arr3[n3];
    int i=0,j=0,k=0;
    
    while(i<n1)
    {
        arr3[k++]=arr1[i++];
    }
    while(j<n2)
    {
        arr3[k++]=arr2[j++];
    }
    for(i=0;i<n3;i++)
    {
        printf("%d ",arr3[i]);
    }
}
